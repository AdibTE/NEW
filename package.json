    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-validator": "^6.6.0",
    "jalali-moment": "^3.3.8",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.3",
    "moment": "^2.27.0",
    "moment-jalaali": "^0.9.2",
    "mongoose": "^5.9.21",
    "nodemailer": "^6.4.16",
    "path": "^0.12.7",
    "persian-date": "^1.1.0",
